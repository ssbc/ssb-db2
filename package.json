  "description": "A new database for SSB",
  "version": "4.2.1",
  "homepage": "https://github.com/ssb-ngi-pointer/ssb-db2",
  "repository": {
    "type": "git",
    "url": "git@github.com:ssb-ngi-pointer/ssb-db2.git"
  },
  "files": [
    "package.json.license",
    "LICENSES/*",
    "*.js",
    "compat/*.js",
    "indexes/*.js",
    "operators/*.js"
  ],
  "dependencies": {
    "async-append-only-log": "^4.3.2",
    "atomic-file-rw": "^0.3.0",
    "binary-search-bounds": "^2.0.4",
    "bipf": "^1.9.0",
    "clarify-error": "^1.0.0",
    "debug": "^4.3.1",
    "fastintcompression": "0.0.4",
    "flumecodec": "0.0.1",
    "flumelog-offset": "3.4.4",
    "hoox": "0.0.1",
    "jitdb": "^7.0.0",
    "level": "^6.0.1",
    "level-codec": "^9.0.2",
    "lodash.debounce": "^4.0.8",
    "mkdirp": "^1.0.4",
    "multicb": "1.2.2",
    "mutexify": "^1.3.1",
    "obz": "^1.1.0",
    "p-defer": "^3.0.0",
    "pull-cat": "^1.1.11",
    "pull-drain-gently": "^1.1.0",
    "pull-level": "^2.0.4",
    "pull-notify": "^0.1.2",
    "pull-paramap": "^1.2.2",
    "pull-stream": "^3.6.14",
    "push-stream": "^11.0.0",
    "rimraf": "^3.0.2",
    "ssb-box": "^1.0.0",
    "ssb-box2": "^2.0.2",
